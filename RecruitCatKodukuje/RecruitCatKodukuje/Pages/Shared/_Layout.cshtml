<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Recruit Cat Kodukuje</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <style bgcolor="grey">
    </style>
</head>
<body class="no-js">
    <div id="wrapper">
        <header id="mainHeader"> </header>
        <a href="/" title="home" class="logo"> </a>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3"></nav>
        <table class="container">
            <a class="navbar-brand" asp-area="" asp-page="/Index">Recruit Cat Kodukuje</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
        </table>
            <table class="table" fontcolor="darkgrey">
                <thead>
                    <tr>
                    <th>
                        <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                    </th>
                    <th>
                        <a class="nav-link text-dark" asp-page="/MathHacks/index">About Us</a>
                    </th>
                    <th>
                        <a class="nav-link text-dark" asp-area="" asp-page="/OurTeam">Our Team</a>
                    </th>
                    <th> 
                        <a class="nav-link text-dark" asp-page="/JobTitles/index">Jobs</a>
                    </th>
                    <th>
                        <a class="nav-link text-dark" asp-area="" asp-page="/ContactUs">Contact Us</a>
                    </th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>


    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - RecruitCatKodukuje - <a asp-area="" asp-page="/Privacy">Privacy</a>
            <a asp-area="" asp-page="/ContactUs">Contact Us</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>